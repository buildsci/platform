        <script>
        function building101DataSubstantial() {
            var form = document.forms.platformForm;

            //BUILDING
            if (form.contains(form.building_name)) {form.building_name.value = "Building 101";}
            if (form.contains(form.weather_epw_location)) {form.weather_epw_location.value = "PHILADELPHIA INTERNATIONAL AP";}
            if (form.contains(form.activity_type)) {form.activity_type.value = "Office";}
            if (form.contains(form.activity_type_specific)) {form.activity_type_specific.value = "Administrative/professional office";}
            
            //ARCHITECTURE
            if (form.contains(form.gross_floor_area)) {form.gross_floor_area.value = 6967.72;} // attic & basement
            if (form.contains(form.building_height)) {form.building_height.value = 15.54;} 
            if (form.contains(form.window_to_wall_ratio)) {form.window_to_wall_ratio.value = 15;}
            if (form.contains(form.number_of_floors)) {form.number_of_floors.value = 4;} //conditioned space, includes basement
            if (form.contains(form.perimeter)) {form.perimeter.value = 800;} 
            if (form.contains(form.footprint_shape)) {form.footprint_shape.value = "T shaped";}
            

            //MATERIALS
            if (form.contains(form.exterior_wall_type)) {form.exterior_wall_type.value = "Brick";}
            if (form.contains(form.window_glass_type)) {form.window_glass_type.value = "Single Layer Glass";} //Windows Double Glazed
            if (form.contains(form.roof_type)) {form.roof_type.value = "none";}

            //PEOPLE
            if (form.contains(form.number_of_employees_during_main_shift)) {form.number_of_employees_during_main_shift.value = 55;}

            //MECHANICAL
            if (form.contains(form.primary_hvac_type)) {form.primary_hvac_type.value = "VAV w/Reheat";}
            if (form.contains(form.demand_control_ventilation)) {form.demand_control_ventilation.value = "no";}
            if (form.contains(form.airside_economizer)) {form.airside_economizer.value = "no";}
            if (form.contains(form.airside_energy_recovery)) {form.airside_energy_recovery.value = "no";}

            //SCHEDULES
            if (form.contains(form.average_weekly_operating_hours)) {form.average_weekly_operating_hours.value = 55;}
            if (form.contains(form.weekday_occupancy_hours_day_start)) {form.weekday_occupancy_hours_day_start.value = "7AM";}
            if (form.contains(form.weekday_occupancy_hours_day_end)) {form.weekday_occupancy_hours_day_end.value = "6PM";}
            if (form.contains(form.saturday_occupancy_hours_day_start)) {form.saturday_occupancy_hours_day_start.value = "closed";}
            if (form.contains(form.saturday_occupancy_hours_day_end)) {form.saturday_occupancy_hours_day_end.value = "closed";}
            if (form.contains(form.sunday_occupancy_hours_day_start)) {form.sunday_occupancy_hours_day_start.value = "closed";}
            if (form.contains(form.sunday_occupancy_hours_day_end)) {form.sunday_occupancy_hours_day_end.value = "closed";}
                        
            
            
        }
        </script>
