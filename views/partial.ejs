        <script>
            function addRow(tableID) {
                var table = document.getElementById(tableID);

                var rowCount = table.rows.length;
                var row = table.insertRow(rowCount - 2);

                var cell1 = row.insertCell(0);
                var element1 = document.createElement("input");
                element1.className = "span2";
                element1.type = "date";
                cell1.appendChild(element1);

                var cell2 = row.insertCell(1);
                var element2 = document.createElement("input");
                element2.type = "number";
                element2.placeholder = "kWh";
                element2.className = "span2";
                cell2.appendChild(element2);

                var cell3 = row.insertCell(2);
                var element3 = document.createElement("input");
                element3.type = "number";
                element3.placeholder = "CCF";
                element3.className = "span2";
                element3.ng - show = "ng";
                cell3.appendChild(element3);

                var cell4 = row.insertCell(3);
                var element4 = document.createElement("input");
                element4.type = "number";
                element4.placeholder = "gallons";
                element4.className = "span1";
                element4.ng - show = "oil";
                cell4.appendChild(element4);

                var cell5 = row.insertCell(4);
                var element5 = document.createElement("input");
                element5.type = "number";
                element5.placeholder = "CCF";
                element5.className = "span1";
                element5.ng - show = "steam";
                cell5.appendChild(element5);
            }

            function deleteRow(tableID) {
                var table = document.getElementById(tableID);
                var rowCount = table.rows.length;
                table.deleteRow(rowCount - 3);
            }
        </script>
