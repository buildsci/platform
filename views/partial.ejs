                        <section id="occupancy ">
                            <div class="box-unit ">
                                <div class="page-header ">
                                    <h2>Occupancy</h2>
                                </div>
                                <div class="row ">
                                    <div class="span4 ">
                                        <h4>Weekday Occupancy Hours</h4>
                                        <h5>Day Start</h5>
                                        <input type="time" ng-model="weekdayStart
                                                " name="occupancy[weekday_occupancy_start] "
                                        />
                                        <h5>Day End</h5>
                                        <input type="time" value={{weekdayStart}} min={{weekdayStart}}
                                        name="occupancy[weekday_occupancy_end]" />
                                    </div>
                                    <div class="span4 ">
                                        <h4>Weekend Occupancy Hours</h4>
                                        <h5>Day Start</h5>
                                        <input type="time" ng-model="weekendStart" name="occupancy[weekend_occupancy_start] "
                                        />
                                        <h5>Day End</h5>
                                        <input type="time" value={{weekendStart}} min={{weekendStart}}
                                        name="occupancy[weekend_occupancy_end]" />
                                        <h4>Number of Occupants</h4>
                                        <input type="number" step="1" min="0 " name="occupancy[number_of_occupants] "
                                        />
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="materials ">
                            <div class="box-unit ">
                                <div class="page-header ">
                                    <h2>Materials</h2>
                                </div>
                                <div class="row ">
                                    <div class="span4 ">
                                        <h4>Walls</h4>
                                        <select class="span3 " name="materials[exterior_wall_type
                                                ] ">
                                            <option>Concrete Mass</option>
                                            <option>Metal</option>
                                            <option>Steel Framed</option>
                                            <option>Wood Framed and Other</option>
                                        </select>
                                        <h4>Windows</h4>
                                        <select class="span3 " name="materials[window_glass_type] ">
                                            <option>Multi-layer Glass</option>
                                            <option>Single Layer Glass</option>
                                            <option>Combination</option>
                                            <option>No Windows</option>
                                            <option>Unknown</option>
                                        </select>
                                        <h4>Thermal Mass</h4>
                                        <select class="span3 " name="materials[thermal_mass] ">
                                            <option>High</option>
                                            <option>Low</option>
                                            <option>None</option>
                                            <option>Isolated in Floor</option>
                                        </select>
                                    </div>
                                    <div class="span4 ">
                                        <h4>Roof</h4>
                                        <select class="span3 " name="materials[roof_type] ">
                                            <option>Modified Bitumen Roof</option>
                                            <option>Metal Roof</option>
                                            <option>Green Roof</option>
                                            <option>Cool Roof</option>
                                        </select>
                                        <h4>Roof Insulation Type</h4>
                                        <select class="span3 " name="materials[roof_insulation_type] ">
                                            <option>Wood Fiber</option>
                                            <option>Perlite Lnsulation Board</option>
                                            <option>Polyisocyanurate</option>
                                            <option>Polystyrene</option>
                                            <option>Extruded Polystyrene</option>
                                            <option>Cellular Glass</option>
                                            <option>Gypsum Board</option>
                                        </select>
                                        <h4>Roof Insulation R Value</h4>
                                        <div class="input-append ">
                                            <input class="span3 " id="appendedInput" type="text" name="materials[roof_insulation_r_value] "
                                            />
                                            <span class="add-on ">m
                                                <sup>2</sup>C/W</span>
                                        </div>
                                        <h4>Roof Insulation Location</h4>
                                        <select class="span3" name="materials[roof_insulation_location]">
                                            <option>Top of Slab</option>
                                            <option>Bottom of Slab</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="mechanical">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Mechanical</h2>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Ventilation System</h4>
                                        <select name="mechanical[ventilation_system]">
                                            <option>Mechanical Cooling & Heating</option>
                                            <option>Natural ventilation cooling, Mechanical Heating</option>
                                            <option>Joint natural ventilation cooling and mechanical heating</option>
                                        </select>
                                    </div>
                                    <div class="span4">
                                        <h4>Equipment Density</h4>
                                        <div class="input-append">
                                            <select class="span2" id="appendedInput" name="mechanical[equipment_power_density]">
                                                <option>0.00 No Equipment</option>
                                                <option>1.00</option>
                                                <option>1.00 Office (Small)</option>
                                                <option>5.00 Office (Medium)</option>
                                                <option>10.00 Office (Large)</option>
                                                <option>15</option>
                                                <option>20</option>
                                                <option>25</option>
                                                <option>50</option>
                                                <option>100</option>
                                            </select>
                                            <span class="add-on">W/m
                                                <sup>2</sup>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
