                        <section id="shape">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Shape/Size</h2>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Gross Floor Area</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.1" name="size[gross_floor_area]"
                                            />
                                            <span class="add-on">m&#178;</span>
                                        </div>
                                        <h4>Number of Floors</h4>
                                        <input type="number" name="size[number_of_floors]"
                                        min="0" step="1" />
                                    </div>
                                    <div class="span4">
                                        <h4>Length</h4>
                                        <div class="input-append">
                                            <input type="number" name="size[building_length]" min="0" step="0.01"
                                            />
                                            <span class="add-on">m</span>
                                        </div>
                                        <h4>Width</h4>
                                        <div class="input-append">
                                            <input type="number" name="size[building_width]" min="0" step="0.01" />
                                            <span class="add-on">m</span>
                                        </div>
                                        <h4>Window/Wall Ratio</h4>
                                        <ul class="inline">
                                            <li>
                                                <input type="range" ng-model="winwall" name="size[window_to_wall_ratio]"
                                                min="0" max="40" step="1" value=50/>
                                            </li>
                                            <li>
                                                <div class="input-append">
                                                    <input class="span1" type="text" value={{winwall}} ng-model="winwall"
                                                    />
                                                    <span class="add-on">%</span>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="span3">
                                        <h4>Shape</h4>
                                        <select name="" ng-model="shapeimage" name="shape[footprint_shape]">
                                            <option value="img/basic_geometry/c_shape_min_zone.png">C Shape</option>
                                            <option value="img/basic_geometry/h_shape_min_zone.png">H Shape</option>
                                            <option value="img/basic_geometry/l_shape_min_zone.png">L Shape</option>
                                            <option value="img/basic_geometry/rect_min_zone.png">Rectangle</option>
                                            <option value="img/basic_geometry/t_shape_min_zone.png">T Shaped</option>
                                            <option value="img/basic_geometry/u_shape_min_zone.png">U Shaped</option>
                                        </select>
                                        <h4>Orientation</h4>
                                        <table>
                                            <tr>
                                                <td></td>
                                                <td></td>
                                                <td>
                                                    <button class="btn" type="button" ng-click="orientation='north'">North</button>
                                                </td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td></td>
                                                <td>
