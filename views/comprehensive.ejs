                    <section id="architecture">
                        <div id="box-unit">
                            <div class="page-header">
                                <h2>Architecture
                                    <small>geometry import by BIMserver</small>
                                </h2>
                            </div>
                            <img src="img/BuildingGEO.jpg" class="img-polaroid">
                            <!--UPLOAD IFC-->
                            <div class="fileupload fileupload-new" data-provides="fileupload">
                                <div class="input-append">
                                    <div class="uneditable-input span3">
                                        <i class="icon-file fileupload-exists"></i>
                                        <span class="fileupload-preview"></span>
                                    </div>
                                    <span class="btn btn-file">
                                        <span class="fileupload-new">Upload Building Model (.IFC)</span>
                                        <span class="fileupload-exists">Change</span>
                                        <input type="file" />
                                    </span>
                                    <a href="#" class="btn fileupload-exists" data-dismiss="fileupload">Remove</a>
                                </div>
                            </div>
                            <!--UPLOAD IFC-->
                            <h2 class="text-right">
                                <small>extracts &
                                    <a href="http://www.virtualpulse.us/V1">visualizes</a> external walls, numbers of floors, gross floor area for simulation</small>
                            </h2>
                        </div>
                    </section>
                    <section>
                        <div class="box-unit">
                            <div class="page-header">
                                <h2>Architecture
                                    <small>typical room</small>
                                </h2>
                            </div>
                            <div class="row">
                                <div class="span2">
                                    <img class="img-rounded" src="img/room_dimensions.png">
                                </div>
                                <div class="span3">
                                    <h4>Room Width</h4>
                                    <div class="input-append">
                                        <input type="number" min="0" step="0.1" class="span2" id="appendedInput"
                                        name="room_width" />
                                        <span class="add-on">m</span>
                                    </div>
                                    <h4>Room Depth</h4>
                                    <div class="input-append">
                                        <input type="number" min="0" step="0.1" class="span2" id="appendedInput"
                                        name="room_depth" />
                                        <span class="add-on">m</span>
                                    </div>
                                    <h4>Room Height</h4>
                                    <div class="input-append">
                                        <input type="number" min="0" step="0.1" class="span2" id="appendedInput"
                                        name="room_height" />
                                        <span class="add-on">m</span>
                                    </div>
                                </div>
                                <div class="span3">
                                    <h4>Window Facade Orientation</h4>
                                    <select name="exterior_shading_orientation"
                                    class="span2">
                                        <option>North</option>
                                        <option>North-East</option>
                                        <option>East</option>
                                        <option>South-East</option>
                                        <option>South</option>
                                        <option>South-West</option>
                                        <option>West</option>
                                        <option>North-West</option>
                                    </select>
                                    <h4>Window Area</h4>
                                    <ul class="inline">
                                        <li>
                                            <input type="range" ng-model="winwalltyp" name="window_to_wall_ratio_room"
                                            min="0" max="40" step="1" value=50/>
                                        </li>
                                        <li>
                                            <div class="input-append">
                                                <input class="span1" type="text" value={{winwalltyp}} ng-model="winwalltyp"
                                                />
                                                <span class="add-on">%</span>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="row">
                                <div class="span2">
                                    <img class="img-rounded" src="img/overhang.gif" width="170">
                                </div>
                                <div class="span3">
                                    <h4>Overhang Depth</h4>
                                    <div class="input-append">
                                        <input class="span2" id="appendedInput" type="number" name="overhang_depth">
                                        <span class="add-on">m</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <section id="materials">
                        <div id="box-unit">
                            <div class="page-header">
                                <h2>Materials</h2>
                            </div>
                            <div class="row">
                                <div class="span4">
                                    <h4>Windows</h4>
                                    <select class="span3">
                                        <option>Multi-layer Glass</option>
                                        <option>Single Layer Clear Glass</option>
                                        <option>Reflective Glass</option>
                                        <option>Dark Tint Glass</option>
                                        <option>Combination</option>
                                        <option>No Windows</option>
                                        <option>Unknown</option>
                                    </select>
                                    <h4>Roof</h4>
                                    <select class="span3">
                                        <option>Insulation Entirely Above Deck</option>
                                        <option>Metal Roof</option>
                                        <option>Attic and Other</option>
                                    </select>
                                </div>
                                <div class="span4">
                                    <h4>Walls</h4>
                                    <select class="span3">
                                        <option>Concrete Mass</option>
                                        <option>Metal</option>
                                        <option>Steel Framed</option>
                                        <option>Wood Framed and Other</option>
                                    </select>
                                    <h4>Wall Thickness</h4>
                                    <div class="input-append">
                                        <input class="span2" id="appendedInput" type="text">
                                        <span class="add-on">m</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <section id="people">
                        <div id="box-unit">
                            <div class="page-header">
                                <h2>People</h2>
                            </div>
                            <div class="row">
                                <div class="span4">
                                    <h4>People Density</h4>
                                    <div class="input-append">
                                        <input class="span2" id="appendedInput" type="text">
                                        <span class="add-on">People/m&sup2;</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <section id="lighting">
                        <div id="box-unit">
                            <div class="page-header">
                                <h2>Lighting
                                    <small>by DAYSIM</small>
                                </h2>
                            </div>
                            <div class="row">
                                <div class="span4">
                                    <h4>Lighting Power Density</h4>
                                    <div class="input-append">
                                        <input class="span2" id="appendedInput" type="text">
                                        <span class="add-on">W/m&sup2;</span>
                                    </div>
                                    <h4>Window Head Height</h4>
                                    <div class="input-append">
                                        <input class="span2" id="appendedInput" type="text" placeholder="floor to top of window">
                                        <span class="add-on">m</span>
                                    </div>
                                    <h4>Shading Device</h4>
                                    <select class="span3">
                                        <option>Horizontal Blinds</option>
                                        <option>Fabric Shades</option>
                                        <option>None</option>
                                    </select>
                                </div>
                                <div class="span4">
                                    <h4>Floor Reflectance</h4>
                                    <input id="floorSlider" type="range" class="span2"
                                    min="0" max="1" step="0.01" onchange="printValue('floorSlider','floorRangeValue')"
                                    />
                                    <input id="floorRangeValue" type="number" class="span1" min="0" max="1"
                                    step="0.01" />
                                    <h4>Ceiling Reflectance</h4>
                                    <input id="ceilingSlider" type="range" class="span2"
                                    min="0" max="1" step="0.01" onchange="printValue('ceilingSlider','ceilingRangeValue')"
                                    />
                                    <input id="ceilingRangeValue" type="number" class="span1" min="0" max="1"
                                    step="0.01" />
                                    <h4>Wall Reflectance</h4>
                                    <input id="wallSlider" type="range" class="span2"
                                    min="0" max="1" step="0.01" onchange="printValue('wallSlider','wallRangeValue')"
                                    />
                                    <input id="wallRangeValue" type="number" class="span1" min="0" max="1"
                                    step="0.01" />
                                </div>
                            </div>
                        </div>
                    </section>
                    <section id="mechanical">
                        <div id="box-unit">
                            <div class="page-header">
                                <h2>Mechanical</h2>
                            </div>
                            <div class="row">
                                <div class="span4">
                                    <h4>Infiltration Rate</h4>
                                    <select class="span3">
                                        <option>Leaky</option>
                                        <option>Average</option>
                                        <option>Tight</option>
                                    </select>
                                </div>
                                <div class="span4">
                                    <h4>Equipment Power Density</h4>
                                    <div class="input-append">
                                        <input class="span2" id="appendedInput" type="text">
                                        <span class="add-on">W/m&sup2;</span>
                                    </div>
                                </div>
                            </div>
                            <h2 class="text-right">
                                <small>HVAC system type & efficiency are based on building type defaults</small>
                            </h2>
                    </section>
                    <section id="schedules">
                        <div id="box-unit">
                            <div class="page-header">
                                <h2>Schedules</h2>
                                <h2>
                                    <small>Occupancy, lighting, equipment schedules are based on building type defaults</small>
                                </h2>
                            </div>
                            <a href="http://www.energycodes.gov/development/commercial/90.1_models">
                            <img src="img/schedules/large_office.png" class="img-polaroid"></a>
                            <h2 class="text-right">
                                <small>based off of
                                    <a href="http://www.energycodes.gov/development/commercial/90.1_models">PNNL's Commercial Prototype Building Models</a>
                                </small>
                            </h2>
                        </div>
                    </section>
                    <section id="conservation">
                        <div id="box-unit">
                            <div class="page-header">
                                <h2>Energy Conservation Measures</h2>
                                <h2>
                                    <small>Choose ECMs to add to your baseline</small>
                                </h2>
                            </div>
                            <label class="checkbox">
                                <input type="checkbox">Lighting Upgrade</label>
                            <label class="checkbox">
                                <input type="checkbox">Window Upgrade</label>
                            <label class="checkbox">
                                <input type="checkbox">Economizer</label>
                        </div>
                    </section>
                    <div id="box-unit">
                        <a href="http://developer.eebhub.org/openstudio/inputs/run_eeb20138202345897.sh.txt"class="btn btn-large btn-primary"/>SIMULATE</a>
                        <!--<input class="btn btn-large btn-primary" type="submit" value="SIMULATE"/></a>-->
                        <button type="button" class="btn btn-large btn-info">
                            <i class="icon-plus"></i>Building 101 Data</button>
                        <a class="btn btn-large btn-info" href="http://128.118.67.241/tools.eebhub.org/energy-use.php"><i class="icon-signal"></i> Example Outputs</a>
                        <h2 class="text-right">
                            <small>powered by OpenStudio SDK + EnergyPlus, DAYSIM, CONTAM Engines</small>
                        </h2>
                    </div>
                </div>
            </div>
            <!-- Javascript -->
            <script src="http://code.jquery.com/jquery.js"></script>
            <script src="js/bootstrap.min.js"></script>
