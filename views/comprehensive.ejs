        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.7/angular.min.js"></script>
        <script>
            function printValue(sliderID, textbox) {
                var x = document.getElementById(textbox);
                var y = document.getElementById(sliderID);
                x.value = y.value;
            }

            window.onload = function() {
                printValue('ceilingSlider', 'ceilingRangeValue');
                printValue('floorSlider', 'floorRangeValue');
                printValue('groundSlider', 'groundRangeValue');
                printValue('wallSlider', 'wallRangeValue');
                printValue('shadingDeviceSlider', 'shadingDeviceRangeValue');
                printValue('overhangSlider', 'overhangRangeValue');
                printValue('exteriorSlider', 'exteriorRangeValue');
                printValue('interiorSlider', 'interiorRangeValue');
                printValue('obstructionSlider', 'obstructionRangeValue');
            }
        </script>
    </head>
    
